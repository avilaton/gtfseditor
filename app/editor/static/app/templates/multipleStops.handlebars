<h4>Selected Stops</h4>
<table class="table table-hover table-condensed">
  <thead>
  <tr>
    <th>Stop-id</th>
    <th>lineas</th>
  </tr>
  </thead>
  <tbody>
  {{#each features}}
    <tr data-stop_id={{fid}}>
      <td><button class="btn btn-mini" id="{{fid}}"><i class="icon-forward"></i></button>{{fid}}</td>
      <td>
      {{#each attributes.stop_lineas}}
      <span>{{this}}, </span>{{/each}}
      </td>
    </tr>
  {{/each}}
  </tbody>
</table>